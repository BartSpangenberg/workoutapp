{{> navbar navBarClasses.workoutLibrary }}
<button id="go-back" class="go-back-button button button-small icon-center"><i class="bi bi-arrow-left"></i></button>
<section class="swap-animation-two section-center">
  

<h1>{{workoutData.name}}</h1>
<h6>{{workoutData.description}}</h6>

    <div class="wo-info-tag-container">
        {{#each tags}}
        <span class="tag">{{this}}</span>
        {{/each}}
    </div>

  <div class="wo-info-ex-container">
    <form method="POST" action="/library/workout-information/{{workoutData._id}}">
      {{#each workoutData.exercises}}
          {{> workoutInfoExercise this}}     
      {{/each}}

      <button>Add to your workouts</button>
    </form>

  </div>

<script src="/js/library/workout-information.js"></script>
</section>